local TabMain = Window:CreateTab("FARM", "dollar-sign")
local SectionMain = TabMain:CreateSection("Main")

local Workspace = game:GetService("Workspace")
local RunService = game:GetService("RunService")
local BredMakurzFolder = Workspace:WaitForChild("Map"):WaitForChild("BredMakurz")

local ESP_Enabled = false
local highlights = {}

-- üü¢ Tworzenie highlightu (raz na model)
local function createHighlight(model)
	if highlights[model] then return end
	local highlight = Instance.new("Highlight")
	highlight.Name = "SafeESP (BUGGY)"
	highlight.FillTransparency = 0.6
	highlight.OutlineTransparency = 0.1
	highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
	highlight.Adornee = model
	highlight.Parent = model
	highlights[model] = highlight
end

-- üî¥ Aktualizacja koloru wg "Broken"
local function updateColor(model)
	local highlight = highlights[model]
	if not highlight then return end
	local values = model:FindFirstChild("Values")
	local broken = values and values:FindFirstChild("Broken")
	if broken and broken:IsA("BoolValue") then
		if broken.Value then
			highlight.FillColor = Color3.fromRGB(255, 0, 0)
			highlight.OutlineColor = Color3.fromRGB(120, 0, 0)
		else
			highlight.FillColor = Color3.fromRGB(0, 255, 0)
			highlight.OutlineColor = Color3.fromRGB(0, 120, 0)
		end
	end
end

-- üßπ Usuwa wszystkie highlighty bez lag√≥w
local function clearESP()
	for model, hl in pairs(highlights) do
		if hl and hl.Parent then
			hl:Destroy()
		end
	end
	table.clear(highlights)
end

-- ‚öôÔ∏è Delikatne od≈õwie≈ºanie (bez lag√≥w)
local refreshIndex = 1
RunService.Stepped:Connect(function()
	if not ESP_Enabled then return end

	local models = BredMakurzFolder:GetChildren()
	if #models == 0 then return end

	refreshIndex += 1
	if refreshIndex > #models then
		refreshIndex = 1
	end

	local model = models[refreshIndex]
	if model and model:IsA("Model") then
		task.defer(function()
			if not ESP_Enabled then return end -- nie r√≥b nic je≈õli wy≈ÇƒÖczone!
			createHighlight(model)
			updateColor(model)
		end)
	end
end)

-- üîÅ ≈öledzenie zmian "Broken" (tylko gdy w≈ÇƒÖczone)
local function trackBrokenValue(model)
	local values = model:FindFirstChild("Values")
	if values then
		local broken = values:FindFirstChild("Broken")
		if broken and broken:IsA("BoolValue") then
			broken:GetPropertyChangedSignal("Value"):Connect(function()
				if ESP_Enabled then
					updateColor(model)
				end
			end)
		end
	end
end

-- üì¶ Nowe modele
BredMakurzFolder.ChildAdded:Connect(function(model)
	if not ESP_Enabled then return end
	if model:IsA("Model") then
		task.defer(function()
			createHighlight(model)
			updateColor(model)
			trackBrokenValue(model)
		end)
	end
end)

-- üß© Toggle GUI
local ESPToggle = TabMain:CreateToggle({
	Name = "Safes ESP",
	CurrentValue = false,
	Flag = "BredMakurzESPToggle",
	Callback = function(Value)
		ESP_Enabled = Value

		if not Value then
			clearESP() -- natychmiastowe wy≈ÇƒÖczenie ESP
		else
			-- w≈ÇƒÖczanie ESP ‚Äì bez lag√≥w
			task.spawn(function()
				for _, model in ipairs(BredMakurzFolder:GetChildren()) do
					if not ESP_Enabled then break end
					if model:IsA("Model") then
						createHighlight(model)
						updateColor(model)
						trackBrokenValue(model)
						task.wait(0.005)
					end
				end
			end)
		end
	end,
})





local Workspace = game:GetService("Workspace")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local LocalPlayer = Players.LocalPlayer

local Prox_Enabled = false
local RefreshRate = 0.1 -- od≈õwie≈ºanie co 0.1s
local Range = 300       -- zasiƒôg dzia≈Çania

local prompts = {}
local lastRefresh = 0

-- üß† Rejestracja prompt√≥w
local function registerPrompt(prompt)
	if not prompt:GetAttribute("OriginalHold") then
		prompt:SetAttribute("OriginalHold", prompt.HoldDuration)
	end
	table.insert(prompts, prompt)
end

-- üì¶ Zbiera wszystkie prompty raz
local function collectPrompts()
	prompts = {}
	for _, obj in ipairs(Workspace:GetDescendants()) do
		if obj:IsA("ProximityPrompt") then
			registerPrompt(obj)
		end
	end
end

-- üß© Reaguje na nowe prompty
Workspace.DescendantAdded:Connect(function(obj)
	if obj:IsA("ProximityPrompt") then
		registerPrompt(obj)
	end
end)

-- ‚öôÔ∏è Ustawia HoldDuration = 0 w zasiƒôgu
local function updateNearbyPrompts()
	local char = LocalPlayer.Character
	if not char then return end
	local root = char:FindFirstChild("HumanoidRootPart")
	if not root then return end

	for _, prompt in ipairs(prompts) do
		if prompt and prompt.Parent and prompt.Parent:IsA("BasePart") then
			local dist = (prompt.Parent.Position - root.Position).Magnitude
			if dist <= Range then
				prompt.HoldDuration = 0
			else
				local original = prompt:GetAttribute("OriginalHold")
				if original then
					prompt.HoldDuration = original
				end
			end
		end
	end
end

-- üß† Lekki loop (co 0.1 sekundy)
RunService.Heartbeat:Connect(function(dt)
	if not Prox_Enabled then return end
	if tick() - lastRefresh > RefreshRate then
		lastRefresh = tick()
		task.spawn(updateNearbyPrompts)
	end
end)

-- üéöÔ∏è Toggle w GUI
local ProximityToggle = TabMain:CreateToggle({
	Name = "Instant Grab",
	CurrentValue = false,
	Flag = "InstantProxNoLag",
	Callback = function(state)
		Prox_Enabled = state
		if state then
			collectPrompts()
			updateNearbyPrompts()
		else
			for _, prompt in ipairs(prompts) do
				if prompt and prompt:GetAttribute("OriginalHold") then
					prompt.HoldDuration = prompt:GetAttribute("OriginalHold")
				end
			end
		end
	end,
})





local TabMain = Window:CreateTab("UPDATE", "bell-plus")
local SectionMain = TabMain:CreateSection("Main")

local Label = TabMain:CreateLabel("NEW BUTTONS ADDED", "bitcoin")
